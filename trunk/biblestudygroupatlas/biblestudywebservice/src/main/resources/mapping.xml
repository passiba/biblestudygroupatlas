<?xml version="1.0"?>
<!DOCTYPE mapping PUBLIC 
    "-//EXOLAB/Castor Object Mapping DTD Version 1.0//EN"
    "http://castor.exolab.org/mapping.dtd">
    
<!-- 
  A Castor mapping file to map EvaluateHandRequest and EvaluateHandResponse
  objects to/from XML.
  
  From Listing 9.4.
 -->
<mapping xmlns="http://castor.exolab.org/">
    <class name="fi.passiba.biblestudy.crosswaybible.webservice.client.jaxb.request.doPassageQueryRequest">
        <map-to xml="doPassageQueryRequest"  
            ns-uri=
            "http://www.springinaction.com/poker/schemas" 
        ns-prefix="tns"/>
        <field name="key" type="string" required="true">
            <bind-xml name="key" node="element"/>
        </field>
        <field name="passage" type="string" required="true">
            <bind-xml name="passage" node="element"/>
        </field>
        <field name="options" type="fi.passiba.biblestudy.crosswaybible.webservice.client.jaxb.request.Options" required="true">
            <bind-xml name="options" node="element" required="true" />
        </field>
    </class>
    <class name="fi.passiba.biblestudy.crosswaybible.webservice.client.jaxb.request.Options" required="true">
        <map-to name="options"/>
        <field name="outputFormat" type="string">
            <bind-xml name="output-format" node="element"/>
        </field>
        <field name="includePassageReferences" type="boolean">
            <bind-xml name="include-passage-references" node="element"/>
        </field>
        <field name="includeFirstVerseNumbers" type="boolean">
            <bind-xml name="include-first-verse-numbers" node="element"/>
        </field>
        <field name="includeVerseNumbers" type="boolean">
            <bind-xml name="include-verse-numbers" node="element"/>
        </field>
        <field name="includeFootnotes" type="boolean">
            <bind-xml name="include-footnotes" node="element"/>
        </field>
        <field name="includeHeadings" type="boolean">
            <bind-xml name="include-headings" node="element"/>
        </field>
        <field name="includeSubheadings" type="boolean">
            <bind-xml name="include-subheadings" node="element"/>
        </field>
        <field name="includeSurroundingChapters" type="boolean">
            <bind-xml name="include-surrounding-chapters" node="element"/>
        </field>
        <field name="includeShortCopyright" type="boolean">
            <bind-xml name="include-short-copyright" node="element"/>
        </field>
        <field name="includeXmlDeclaration" type="boolean">
            <bind-xml name="include-xml-declaration" node="element"/>
        </field>
        <field name="includeDoctype" type="boolean">
            <bind-xml name="include-doctype" node="element"/>
        </field>
        <field name="includeQuoteEntities" type="boolean">
            <bind-xml name="include-quote-entities" node="element"/>
        </field>
        <field name="includeSimpleEntities" type="boolean">
            <bind-xml name="include-simple-entities" node="element"/>
        </field>
        <field name="includeCrossReferences" type="boolean">
            <bind-xml name="include-cross-references" node="element"/>
        </field>
        <field name="includeLineBreaks" type="boolean">
            <bind-xml name="include-line-breaks" node="element"/>
        </field>
        <field name="includeVirtualAttributes" type="boolean">
            <bind-xml name="include-virtual-attributes" node="element"/>
        </field>
        <field name="baseElement" type="string">
            <bind-xml name="base-element" node="element"/>
        </field>
        <field name="includePassageHorizontalLines" type="boolean">
            <bind-xml name="include-passage-horizontal-lines" node="element"/>
        </field>
        <field name="includeHeadingHorizontalLines" type="boolean">
            <bind-xml name="include-heading-horizontal-lines" node="element"/>
        </field>
        <field name="includeSelahs" type="boolean">
            <bind-xml name="include-selahs" node="element"/>
        </field>
        <field name="includeWordIds" type="boolean">
            <bind-xml name="include-word-ids" node="element"/>
        </field>
        <field name="includeContentType" type="boolean">
            <bind-xml name="include-content-type" node="element"/>
        </field>
        <field name="lineLength" type="integer">
            <bind-xml name="line-length" node="element"/>
        </field>
    </class>
    <class name="fi.passiba.biblestudy.crosswaybible.webservice.client.jaxb.response.DoPassageQueryResponse">
        <map-to xml="doPassageQueryResponse"  
        ns-uri=
            "http://www.crosswaybibles.org/soap/"
        ns-prefix="tns" />
        <!--<field name="_return"
        type="fi.passiba.biblestudy.crosswaybible.webservice.client.jaxb.response.Return" required="true">
            <bind-xml name="return" node="element"/>
        </field>-->
    </class>
